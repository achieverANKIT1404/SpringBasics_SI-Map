<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"  
			    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
			    xmlns:p="http://www.springframework.org/schema/p"  
			    xsi:schemaLocation="http://www.springframework.org/schema/beans  
		        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean  id="jbs" class="MyPack.Jobs">
        <property name="srNo" value="1"></property>
        <property name="jobType" value="Web Developer"></property>
        <property name="jobs">
            <map>
                <entry key="Java Developer" value="Aman Shah"></entry>
                <entry key="C# Developer" value="Nitish Kumar"></entry>
            </map>
        </property>
    </bean>
    
</beans>